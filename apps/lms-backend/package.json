{
  "name": "leave-menagement-service",
  "version": "1.0.0",
  "description": "Leave Management Service",
  "main": "server.js",
  "scripts": {
    "setup": "npm install && npm run db:create && npm run up && npm run seed",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "lint:fix": "eslint --fix .",
    "test": "jest --config=jest.config.js --forceExit --watchAll",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "up": "npx sequelize-cli db:migrate",
    "undo": "npx sequelize-cli db:migrate:undo",
    "down": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo",
    "seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "handle-leave-balance": "node commands/handle-leave-balance.js"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-plugin-snakecasejs": "^2.2.0",
    "nodemon": "^3.1.4"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "bcrypt": "^6.0.0",
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "holidayapi": "^6.0.1",
    "ioredis": "^5.4.1",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "ora-classic": "^5.4.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.1",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3",
    "umzug": "^3.8.2",
    "uuid": "^9.0.1",
    "nodemon": "^3.1.4"
  }
}
